{
  "$id": "http://purl.allotrope.org/json-schemas/adm/flow-cytometry/BENCHLING/2023/12/flow_cytometry.schema",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "flow cytomeytry aggregate document": {
      "$asm.property-class": "",
      "$asm.pattern": "aggregate datum",
      "type": "object",
      "properties": {
        "flow cytometry document": {
          "$asm.property-class": "",
          "$asm.pattern": "aggregate datum",
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "measurement aggregate document": {
                "$asm.property-class": "http://purl.allotrope.org/ontologies/result#AFR_0002374",
                "$asm.pattern": "aggregate datum",
                "type": "object",
                "properties": {
                  "measurement document": {
                    "$asm.property-class": "http://purl.allotrope.org/ontologies/result#AFR_0002375",
                    "$asm.array-ordered": false,
                    "$asm.array-mixed": false,
                    "$asm.pattern": "indexed datum",
                    "type": "array",
                    "items": {
                      "$asm.pattern": "aggregate datum",
                      "type": "object",
                      "properties": {
                        "experimental data identifier": {
                          "type": "string"
                        },
                        "processed data aggregate document": {
                          "$asm.property-class": "http://purl.allotrope.org/ontologies/result#AFR_0002658",
                          "type": "object",
                          "properties": {
                            "processed data document": {
                              "$asm.property-class": "http://purl.allotrope.org/ontologies/result#AFR_0002659",
                              "type": "array",
                              "items": {
                                "type": "object",
                                "properties": {
                                  "data processing document": {
                                    "$asm.property-class": "http://purl.allotrope.org/ontologies/result#AFR_0002655",
                                    "type": "object",
                                    "properties": {
                                      "data processing identifier": {
                                        "type": "string"
                                      }
                                    }
                                  },
                                  "event population aggregate document": {
                                    "type": "object",
                                    "properties": {
                                      "event population document": {
                                        "type": "array",
                                        "items": {
                                          "type": "object",
                                          "properties": {
                                            "population identifier": {
                                              "type": "string"
                                            },
                                            "population name": {
                                              "type": "string"
                                            },
                                            "population size": {
                                              "type": "object",
                                              "properties": {
                                                "value": {
                                                  "type": "number"
                                                },
                                                "unit": {
                                                  "type": "string"
                                                }
                                              }
                                            },
                                            "population percentage of parentage": {
                                              "type": "object",
                                              "properties": {
                                                "parental parent identifier": {
                                                  "type": "string"
                                                },
                                                "proportion size": {
                                                  "type": "object",
                                                  "properties": {
                                                    "value": {
                                                      "type": "number"
                                                    },
                                                    "unit": {
                                                      "type": "string"
                                                    }
                                                  }
                                                }
                                              }
                                            }
                                          }
                                        }
                                      }
                                    }
                                  }
                                }
                              }
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
